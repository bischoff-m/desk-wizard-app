{
    "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
    "build": {
        "beforeBuildCommand": "npm run build",
        "beforeDevCommand": "npm run dev",
        "frontendDist": "../out",
        "devUrl": "http://localhost:23553/native-app"
    },
    "bundle": {
        "publisher": "Marco Bischoff",
        "active": true,
        "category": "Utility",
        "copyright": "GNU General Public License v3.0",
        "targets": "msi",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "resources": [
            "resources/**/*"
        ]
    },
    "productName": "Desk Wizard",
    "mainBinaryName": "desk-wizard",
    "version": "0.1.0",
    "identifier": "com.bischoff-m.desk-wizard",
    "app": {
        "windows": [
            {
                "fullscreen": false,
                "height": 600,
                "resizable": true,
                "title": "DeskWizard",
                "width": 800,
                "visible": false
            }
        ],
        "security": {
            "csp": null
        }
    }
}